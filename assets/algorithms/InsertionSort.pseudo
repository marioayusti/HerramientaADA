/*--- Algoritmo de ordenamiento por inserci√≥n o de la baraja (InsertionSort) */

procedure InsertionSort(ES int[] A)
var
int i,j,x
{
    for i<-2 to A.len do 
        x <- A[i]
        j <- i - 1
        while ((j>0) and (A[j]>x)) do 
            A[j+1] <- A[j]
            j <- j - 1
        endwhile
        A[j+1] <- x
    endfor
}

procedure main()
var
int[9] A <- [100,24,23,4,5,6,7,8,9] // Ingresar el arreglo
{
    InsertionSort(A)
}